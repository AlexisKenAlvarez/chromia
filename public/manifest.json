{
  "manifest_version": 3,
  "name": "HandyPlay",
  "version": "1.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage",
    "nativeMessaging"
  ],
  "options_page": "options.html",
  "host_permissions": ["http://*/*", "https://*/*"]

}
