{
  "manifest_version": 3,
  "name": "Thesis",
  "version": "1.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "browser_action": {
    "deault_icon": "favicon-32x32.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage",
    "nativeMessaging",
    "unlimitedStorage",
    "sidePanel",
    "webNavigation"
  ],
  "options_page": "options.html",
  "host_permissions": ["http://*/*", "https://*/*"]
}
