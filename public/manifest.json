{
  "manifest_version": 3,
  "name": "Chromia",
  "version": "1.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "host_permissions": ["http://*/*", "https://*/*"]
}
